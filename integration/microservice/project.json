{
  "root": "integration/microservice",
  "sourceRoot": "integration/microservice/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/integration/microservice",
        "main": "integration/microservice/src/main.ts",
        "tsConfig": "integration/microservice/tsconfig.app.json",
        "assets": ["integration/microservice/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "integration/microservice/src/environments/environment.ts",
              "with": "integration/microservice/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "microservice:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["integration/microservice/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/integration/microservice"],
      "options": {
        "jestConfig": "integration/microservice/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
